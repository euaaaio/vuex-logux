{
  "name": "@logux/vuex",
  "version": "0.3.0",
  "description": "Vuex compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "vuex",
    "websocket"
  ],
  "scripts": {
    "spell": "yaspeller *.md",
    "clean": "rimraf coverage/",
    "test": "jest --coverage && eslint . && check-dts && size-limit && yarn spell"
  },
  "author": "Eduard Aksamitov <e@edaks.co>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "logux/vuex",
  "sideEffects": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "vue": ">=2.6.11",
    "vuex": ">=3.0.0"
  },
  "dependencies": {
    "@logux/client": "^0.8.4",
    "@logux/core": "^0.5.3",
    "nanoevents": "^5.1.5"
  },
  "devDependencies": {
    "@logux/eslint-config": "^36.1.2",
    "@logux/sharec-config": "^0.6.2",
    "@size-limit/preset-small-lib": "^4.4.5",
    "@vue/test-utils": "^1.0.0-beta.33",
    "check-dts": "^0.3.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^18.0.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.6",
    "nanodelay": "^1.0.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.4.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-template-compiler": "^2.6.11",
    "vue-typed-mixins": "^0.2.0",
    "vuex": "^3.1.3",
    "yaspeller": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "size-limit": [
    {
      "import": "{ createLogux, subscriptionMixin, subscriptionComponent }",
      "limit": "10 KB"
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)"
    ],
    "dictionary": [
      "versioning",
      "peerDependencies",
      "npm",
      "vue",
      "GraphQL",
      "io",
      "logux",
      "vuex",
      "mixin",
      "TypeDoc",
      "WebSocket"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.6.2"
  }
}
