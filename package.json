{
  "name": "@logux/vuex",
  "version": "0.8.4",
  "description": "Vuex compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "vuex",
    "websocket"
  ],
  "scripts": {
    "test": "jest --coverage && eslint . && check-dts && size-limit && yaspeller *.md"
  },
  "author": "Eduard Aksamitov <e@edaks.co>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "logux/vuex",
  "types": "./index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": "^12.0.0 || >=14.0.0"
  },
  "peerDependencies": {
    "vue": ">=2.6.12",
    "vuex": ">=3.6.2"
  },
  "dependencies": {
    "@logux/client": "^0.9.3",
    "@logux/core": "^0.6.2",
    "nanoevents": "^5.1.13"
  },
  "devDependencies": {
    "@logux/eslint-config": "^44.1.2",
    "@logux/sharec-config": "^0.6.2",
    "@size-limit/dual-publish": "^4.10.1",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@vue/test-utils": "^1.1.3",
    "check-dts": "^0.4.4",
    "dual-publish": "^1.0.5",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.17.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nanodelay": "^1.0.8",
    "size-limit": "^4.10.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-template-compiler": "^2.6.12",
    "vue-typed-mixins": "^0.2.0",
    "vuex": "^3.6.2",
    "yaspeller": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "rules": {
      "prettierx/options": "off"
    }
  },
  "size-limit": [
    {
      "name": "Store",
      "import": "{ createLogux }",
      "limit": "10 KB"
    },
    {
      "name": "Store + Helpers",
      "import": "{ createLogux, loguxMixin, loguxComponent }",
      "limit": "11 KB"
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)"
    ],
    "dictionary": [
      "versioning",
      "peerDependencies",
      "npm",
      "vue",
      "GraphQL",
      "io",
      "logux",
      "vuex",
      "mixin",
      "mixinâ€™s",
      "TypeDoc",
      "WebSocket",
      "payload",
      "ES",
      "roadmap",
      "refactor",
      "nanoevents"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.6.2"
  }
}
