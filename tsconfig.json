{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strictFunctionTypes": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "stripInternal": true,
    "allowJs": true,
    "target": "esnext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "module": "commonjs"
  },
  "exclude": [
    "**/errors.ts"
  ]
}
