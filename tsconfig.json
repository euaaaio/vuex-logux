{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strictFunctionTypes": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "stripInternal": true,
    "allowJs": true,
    "target": "ES5",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
